###  http://www.semanticweb.org/ontologies/2025/0/teman-klinik/BahanAktif
:BahanAktif rdf:type owl:Class ;
    owl:disjointWith :Gejala, :KelompokPasien, :Obat, :Pasien, :Penyakit ;
    rdfs:label "Bahan Aktif"@id ;
    rdfs:comment "Zat aktif yang terkandung dalam obat"@id .

###  http://www.semanticweb.org/ontologies/2025/0/teman-klinik/Gejala
:Gejala rdf:type owl:Class ;
    owl:disjointWith :KelompokPasien, :Obat, :Pasien, :Penyakit ;
    rdfs:label "Gejala"@id ;
    rdfs:comment "Tanda-tanda atau keluhan yang dirasakan pasien"@id .

###  http://www.semanticweb.org/ontologies/2025/0/teman-klinik/KelompokPasien
:KelompokPasien rdf:type owl:Class ;
    owl:disjointWith :Obat, :Pasien, :Penyakit ;
    rdfs:label "Kelompok Pasien"@id ;
    rdfs:comment "Kategori pasien berdasarkan kondisi khusus"@id .

###  http://www.semanticweb.org/ontologies/2025/0/teman-klinik/Obat
:Obat rdf:type owl:Class ;
    rdfs:subClassOf [ rdf:type owl:Restriction ;
                      owl:onProperty :mengandungBahanAktif ;
                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                      owl:onClass :BahanAktif ] ;
    owl:disjointWith :Pasien, :Penyakit ;
    rdfs:label "Obat"@id ;
    rdfs:comment "Produk farmasi untuk mengobati penyakit"@id .

###  http://www.semanticweb.org/ontologies/2025/0/teman-klinik/Pasien
:Pasien rdf:type owl:Class ;
    rdfs:subClassOf [ rdf:type owl:Restriction ;
                      owl:onProperty :memilikiPenyakit ;
                      owl:maxQualifiedCardinality "10"^^xsd:nonNegativeInteger ;
                      owl:onClass :Penyakit ] ;
    owl:disjointWith :Penyakit ;
    rdfs:label "Pasien"@id .

###  http://www.semanticweb.org/ontologies/2025/0/teman-klinik/Penyakit
:Penyakit rdf:type owl:Class ;
    rdfs:subClassOf [ rdf:type owl:Restriction ;
                      owl:onProperty :dapatDiobatiDengan ;
                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                      owl:onClass :Obat ],
                    [ rdf:type owl:Restriction ;
                      owl:onProperty :memilikiGejala ;
                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                      owl:onClass :Gejala ] ;
    rdfs:label "Penyakit"@id .

###  http://www.semanticweb.org/ontologies/2025/0/teman-klinik/EfekSamping
:EfekSamping rdf:type owl:Class ;
